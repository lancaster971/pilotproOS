# Intelligence Engine Environment Variables

# Service Configuration
SERVICE_MODE=all
API_PORT=8000
UI_PORT=8501

# Database
DB_HOST=postgres-dev
DB_PORT=5432
DB_NAME=pilotpros_db
DB_USER=pilotpros_user
DB_PASSWORD=pilotpros_secure_pass_2025

# Redis
REDIS_HOST=redis-dev
REDIS_PORT=6379

# LangChain (optional - for tracing)
LANGCHAIN_TRACING_V2=true
LANGCHAIN_PROJECT=pilotpros-intelligence
LANGCHAIN_API_KEY=your-langsmith-api-key-here

# LLM API Keys (required for operation)
OPENAI_API_KEY=your-openai-api-key
ANTHROPIC_API_KEY=your-anthropic-api-key
GROQ_API_KEY=your-groq-api-key

# n8n Integration
N8N_URL=http://automation-engine-dev:5678
N8N_API_KEY=your-n8n-api-key

# Monitoring
ENABLE_MONITORING=true
LOG_LEVEL=INFO